@{
	ViewData["Title"] = "Index";
	Layout = "~/Views/Shared/_AdminLayout.cshtml";
}

@*@{
	Layout = null;
}*@ @* Burada hata düzeliyor açarsan. Sorun AdminLayout ile ilgili oraya bak düzelt.*@

<!DOCTYPE html>
<html lang="en"
	  class="light-style layout-menu-fixed"
	  dir="ltr"
	  data-theme="theme-default"
	  data-assets-path="~/sneat/assets/"
	  data-template="vertical-menu-template-free">
<head>
	<meta charset="utf-8" />
	<meta name="viewport"
		  content="width=device-width, initial-scale=1.0, user-scalable=no, minimum-scale=1.0, maximum-scale=1.0" />

	<title>Admin Dashboard</title>

	<meta name="description" content="" />

	<!-- Favicon -->
	<link rel="icon" type="~/sneat/image/x-icon" href="~/sneat/assets/img/favicon/favicon.ico" />


</head>

<body>
	<div class="container-xxl flex-grow-1 container-p-y">
		<div class="row">
			<div class="col-lg-12 mb-4 order-0">
				<div class="card">
					<div class="d-flex align-items-end row">
						<div class="col-sm-7">
							<div class="card-body">
								<h5 class="card-title text-primary">Selamlar @ViewBag.v1! 🎉</h5>
								<p class="mb-4">
									Profilindeki tamamlanmamış alan durumu <span class="fw-bold" style="color:seagreen">%@ViewBag.v2</span>! @ViewBag.v3
								</p>

								<a href="/Profile/" class="btn btn-sm btn-outline-primary">Profili Görüntüle</a>
							</div>
						</div>
						<div class="col-sm-5 text-center text-sm-left">
							<div class="card-body pb-0 px-0 px-md-4">
								<img src="~/sneat/assets/img/illustrations/man-with-laptop-light.png"
									 height="140"
									 alt="View Badge User"
									 data-app-dark-img="illustrations/man-with-laptop-dark.png"
									 data-app-light-img="illustrations/man-with-laptop-light.png" />
							</div>
						</div>
					</div>
				</div>
			</div>
			<div class="col-12 col-lg-8 order-2 order-md-3 order-lg-2 mb-4">
				<div class="card">
					<div class="row row-bordered g-0">
						<div class="col-md-8">
							<h5 class="card-header m-0 me-2 pb-3">Doldurulacak</h5>
							@* -> Here <- *@
						</div>
						<div class="col-md-4">
							<div class="card-body">
								<div class="text-center">
									
								</div>
							</div>
							<div id="growthChart"></div>
							<div class="text-center fw-semibold pt-3 mb-2">62% İçerik Oranı</div>
						</div>
					</div>
				</div>
			</div>
			<div class="col-18 col-md-8 col-lg-4 order-3 order-md-2">
				<div class="row">
					<div class="col-6 mb-4">
						<div class="card">
							<div class="card-body">
								<div class="card-title d-flex align-items-start justify-content-between">
									<div class="avatar flex-shrink-0">
										<img src="~/sneat/assets/img/icons/unicons/socialmedia.png" alt="Social Media" class="rounded" />
									</div>
								</div>
								<span class="d-block mb-1">Sosyal Medyalar</span>
								<h3 class="card-title text-nowrap mb-2">@ViewBag.v4</h3>
							</div>
						</div>
					</div>
					<div class="col-6 mb-4">
						<div class="card">
							<div class="card-body">
								<div class="card-title d-flex align-items-start justify-content-between">
									<div class="avatar flex-shrink-0">
										<img src="~/sneat/assets/img/icons/unicons/hobby.png" alt="Hobby" class="rounded" />
									</div>
								</div>
								<span class="fw-semibold d-block mb-1">İlgi Alanları</span>
								<h3 class="card-title mb-2">@ViewBag.v5</h3>
							</div>
						</div>
					</div>
					<div class="col-6 mb-4">
						<div class="card">
							<div class="card-body">
								<div class="card-title d-flex align-items-start justify-content-between">
									<div class="avatar flex-shrink-0">
										<img src="~/sneat/assets/img/icons/unicons/award.png" alt="Awards" class="rounded" />
									</div>
								</div>
								<span class="fw-semibold d-block mb-1">Ödüller</span>
								<h3 class="card-title mb-2">@ViewBag.v6</h3>
							</div>
						</div>
					</div>
					<div class="col-6 mb-4">
						<div class="card">
							<div class="card-body">
								<div class="card-title d-flex align-items-start justify-content-between">
									<div class="avatar flex-shrink-0">
										<img src="~/sneat/assets/img/icons/unicons/education.png" alt="Education" class="rounded" />
									</div>
								</div>
								<span class="fw-semibold d-block mb-1">Eğitimler</span>
								<h3 class="card-title mb-2">@ViewBag.v7</h3>
							</div>
						</div>
					</div>
				</div>

			</div>
		</div>
		<div class="layout-overlay layout-menu-toggle"></div>

	</div>
</body>
</html>
